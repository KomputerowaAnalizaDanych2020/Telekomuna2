<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="background">
    <property name="height">200</property>
    <property name="padding">10</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Label" id="header">
        <property name="font">{times new roman} 16 {bold}</property>
        <property name="justify">center</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">XMODEM</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
          <property id="0" name="pad" type="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="chooseFile">
        <property name="height">300</property>
        <property name="width">400</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="text1">
            <property name="text" translatable="yes">Odczyt z pliku:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Menubutton" id="choiceCOM">
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">COM</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">e</property>
            </layout>
            <child>
              <object class="tk.Menu" id="menu1">
                <child>
                  <object class="tk.Menuitem.Command" id="COM1button">
                    <property cbtype="simple" name="command" type="command">choiceCOM1</property>
                    <property name="label" translatable="yes">COM1</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="COM2button">
                    <property cbtype="simple" name="command" type="command">choiceCOM2</property>
                    <property name="label" translatable="yes">COM2</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="COM3button">
                    <property cbtype="simple" name="command" type="command">choiceCOM3</property>
                    <property name="label" translatable="yes">COM3</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="text2">
            <property name="text" translatable="yes">Port COM:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button3">
            <property cbtype="simple" name="command" type="command">loadFile</property>
            <property name="compound">top</property>
            <property name="text" translatable="yes">Wybierz plik</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">5</property>
              <property name="propagate">False</property>
              <property name="row">1</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="text3">
            <property name="text" translatable="yes">Zapis do pliku:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="propagate">False</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="text4">
            <property name="text" translatable="yes">Tryb:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Menubutton" id="choiceMode">
            <property name="text" translatable="yes">Tryb</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">e</property>
            </layout>
            <child>
              <object class="tk.Menu" id="menu2">
                <child>
                  <object class="tk.Menuitem.Command" id="CRCbutton">
                    <property cbtype="simple" name="command" type="command">choiceCRC</property>
                    <property name="label" translatable="yes">CRC</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="Checksum">
                    <property cbtype="simple" name="command" type="command">choicechecksum</property>
                    <property name="label" translatable="yes">Suma kontrolna</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button5">
            <property cbtype="simple" name="command" type="command">saveFile</property>
            <property name="text" translatable="yes">Wybierz plik</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="text5">
            <property name="text" translatable="yes">Bity stopu:
</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Menubutton" id="choiceBits">
            <property name="text" translatable="yes">Bit stopu</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">e</property>
            </layout>
            <child>
              <object class="tk.Menu" id="menu3">
                <child>
                  <object class="tk.Menuitem.Command" id="oneButton">
                    <property cbtype="simple" name="command" type="command">one</property>
                    <property name="label" translatable="yes">1</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="onePointFiveButton">
                    <property cbtype="simple" name="command" type="command">onePointFive</property>
                    <property name="label" translatable="yes">1.5</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="twoButton">
                    <property cbtype="simple" name="command" type="command">two</property>
                    <property name="label" translatable="yes">2</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label2">
            <property name="text" translatable="yes">Szerokosc pasma</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="baudrate">
            <property name="text" translatable="yes">9600</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="authors">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
        <child>
          <object class="ttk.Label" id="text6">
            <property name="text" translatable="yes">Autorzy: Daniel Malicki, Maciej Wlodarczyk</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="logs">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="logsTitle">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Label" id="text7">
                <property name="text" translatable="yes">Logi:
</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="logsContent">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="tk.Text" id="text8">
                <property name="autoseparators">false</property>
                <property name="blockcursor">false</property>
                <property name="font">TkDefaultFont</property>
                <property name="height">10</property>
                <property name="state">normal</property>
                <property name="width">32</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
        <child>
          <object class="ttk.Button" id="sendButton">
            <property cbtype="simple" name="command" type="command">sendFile</property>
            <property name="text" translatable="yes">Wyslij</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame3">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
        <child>
          <object class="ttk.Button" id="receiveButton">
            <property cbtype="simple" name="command" type="command">receiveFile</property>
            <property name="text" translatable="yes">Odbierz</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
